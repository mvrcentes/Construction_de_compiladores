fun factorial(n) {
  if (n <= 1) return 1;
  return n * factorial(n - 1);
}

print "Factorial de 5: " + factorial(5); // Salida: Factorial de 5: 120

fun fibonacci(n) {
  if (n <= 1) return n;
  return fibonacci(n - 1) + fibonacci(n - 2);
}

print "Fibonacci de 10: " + fibonacci(10); // Salida: Fibonacci de 10: 55