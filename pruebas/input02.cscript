fun ackermann(m, n) {
  if (m == 0) {
    return n + 1;
  }
  if (m > 0 and n == 0) {
    return ackermann(m - 1, 1);
  } 
  if (m > 0 and n > 0) {
    var a = ackermann(m, n - 1);
    return ackermann(m - 1, a);
  }
}

// Testing the Ackermann function
var m = 2;
var n = 3;
var result = ackermann(m, n);
print "Ackermann(" + m + ", " + n + ") = " + result;

